{
	"name": "itnav-hp",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite build && vite preview",
		"deploy": "vite build && gh-pages -d build -t true",
		"test": "playwright test",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"type-check": "tsc --noEmit",
		"lint": "eslint . && stylelint \"**/*.{css,scss,sass,svelte}\" && prettier --plugin-search-dir . --check .",
		"format": "eslint --fix . && stylelint --fix \"**/*.{css,scss,sass,svelte}\" && prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@sveltejs/kit": "1.15.2",
		"@sveltestack/svelte-query": "^1.6.0",
		"svelte": "^3.58.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "2.0.1",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.7",
		"prettier-plugin-jsdoc": "^0.4.2",
		"prettier-plugin-organize-attributes": "^0.0.5",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.61.0",
		"stylelint": "^15.4.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recess-order": "^4.0.0",
		"stylelint-config-recommended-scss": "^9.0.1",
		"stylelint-declaration-block-no-ignored-properties": "^2.7.0",
		"stylelint-prettier": "^3.0.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"svelte-sitemap": "^2.6.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vitest": "^0.30.0"
	},
	"type": "module"
}
